{
    "name": "wucdbm/quick-ui-bundle",
    "type": "symfony-bundle",
    "description": "Quick UI helpers for Symfony projects",
    "keywords": [
        "symfony ui",
        "symfony quick ui",
        "wucdbm"
    ],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "~4.0|~5.0",
        "symfony/form": "~4.0|~5.0",
        "symfony/doctrine-bridge": "~4.0|~5.0",
        "doctrine/orm": "~2.7",
        "twig/twig": "~2.0|~3.0"
    },
    "require-dev": {
        "wucdbm/php-cs-fixers": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Wucdbm\\Bundle\\QuickUIBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}
